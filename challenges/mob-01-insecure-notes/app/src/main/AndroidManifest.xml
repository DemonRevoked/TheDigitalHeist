<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Intentionally OFFLINE-ONLY: no INTERNET permission -->

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TDH.Mob01">

        <!-- Part of the access phrase lives in manifest metadata (easy reversing breadcrumb) -->
        <meta-data
            android:name="mob01_phrase_part_manifest"
            android:value="MINT" />

        <activity
            android:name="com.tdhctf.mob01.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>


